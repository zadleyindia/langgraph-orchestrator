# Test Environment for Supergateway Memory Integration
# Copy your real credentials here for testing

# REQUIRED: OpenAI API Key
OPENAI_API_KEY=your-openai-api-key-here

# REQUIRED: Supabase Configuration
SUPABASE_URL=your-supabase-project-url
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# OPTIONAL: Direct Database Connection (if not using Supabase)
DATABASE_PASSWORD=your-database-password
DATABASE_URL=your-database-connection-string

# Feature Configuration
ENABLE_SMART_UPDATES=true
ENABLE_RPC_CACHE=true
ENABLE_AGENT_CONTEXT=true

# Memory Limits
MAX_OBSERVATIONS_PER_ENTITY=1000
MAX_SEARCH_RESULTS=50

# Environment
NODE_ENV=development
LOG_LEVEL=INFO
REQUIRE_SSL=false